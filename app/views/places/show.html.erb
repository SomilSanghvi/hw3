<h1><%= @place["name"] %> </h1>

<% @entries.each do |entry| %> 
<h3><%= entry["title"] %> </h3>
  <p> <%= entry["occurred_on"] %> 
    </p>
  <p> <%= entry["description"] %> 
    </p>
<% end %>

<p>
  <a href= "/entries/new?place_id=<%= @place["id"] %>">New Entry</a>
</p>

<p>
  <a href="/places">Back to All Places</a>
</p>